{
  "name": "total-war-script-highlighting",
  "displayName": "Total War Script Highlighting",
  "description": "Highlighting support for the scripting language/data format used in Rome Total War",
  "publisher": "RenatoPereira",
  "version": "0.0.1",
  "icon": "icon.png",
  "galleryBanner.color": "00617E",
  "pricing": "Free",
  "repository": {
    "url": "https://github.com/zeus-nikephoros/total-war-script-highlighting"    
  },
  "engines": {
    "vscode": "^1.73.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "total_war_script",
      "aliases": ["Total War Script", "total_war_script"],
      "extensions": [".txt"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "total_war_script",
      "scopeName": "source.total_war_script",
      "path": "./syntaxes/total_war_script.tmLanguage.json"
    }]
  }
}
